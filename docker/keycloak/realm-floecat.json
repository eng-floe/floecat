{
  "realm": "floecat",
  "enabled": true,
  "displayName": "Floecat",
  "sslRequired": "none",
  "roles": {
    "realm": [
      {
        "name": "administrator"
      }
    ]
  },
  "clients": [
    {
      "clientId": "floecat-client",
      "enabled": true,
      "publicClient": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "secret": "floecat-secret",
      "fullScopeAllowed": true,
      "standardFlowEnabled": true,
      "defaultClientScopes": [
        "roles"
      ],
      "redirectUris": [
        "*"
      ],
      "webOrigins": [
        "*"
      ],
      "protocolMappers": [
        {
          "name": "audience",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "floecat-client",
            "access.token.claim": "true",
            "id.token.claim": "true"
          }
        },
        {
          "name": "account_id",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-hardcoded-claim-mapper",
          "consentRequired": false,
          "config": {
            "claim.name": "account_id",
            "claim.value": "21232f29-7a57-35a7-8389-4a0e4a801fc3",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        },
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "claim.name": "roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "id.token.claim": "true",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "email": "admin@floecat.local",
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "requiredActions": [],
      "realmRoles": [
        "administrator"
      ]
    },
    {
      "username": "service-account-floecat-client",
      "enabled": true,
      "serviceAccountClientId": "floecat-client",
      "requiredActions": [],
      "realmRoles": [
        "administrator"
      ]
    }
  ]
}
