{
  "id": null,
  "uid": null,
  "title": "Floecat Telemetry Hub \u2014 Demo",
  "schemaVersion": 40,
  "version": 1,
  "tags": [
    "telemetry",
    "floecat",
    "demo"
  ],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m"
    ],
    "time_options": [
      "5m",
      "15m",
      "30m",
      "1h",
      "3h"
    ]
  },
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "label": "Prometheus datasource",
        "query": "prometheus",
        "current": {
          "text": "prometheus",
          "value": "prometheus"
        }
      },
      {
        "name": "rate_interval",
        "type": "interval",
        "label": "Rate window",
        "query": "30s,1m,5m",
        "current": {
          "text": "1m",
          "value": "1m"
        }
      },
      {
        "name": "component",
        "type": "query",
        "label": "Component",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(floecat_core_rpc_requests_total, component)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": ".*"
        }
      },
      {
        "name": "operation",
        "type": "query",
        "label": "Operation",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(floecat_core_rpc_requests_total{component=~\"$component\"}, operation)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "text",
      "title": "Floecat Telemetry Hub (demo)",
      "id": 1,
      "gridPos": { "h": 3, "w": 24, "x": 0, "y": 0 },
      "options": {
        "mode": "markdown",
        "content": "### Floecat Telemetry Hub\n**At-a-glance health for DBAs & devs**. Metrics via Prometheus at `/q/metrics`, Grafana at `http://localhost:3000`. Optional OTLP: enable `telemetry.exporters=prometheus,otlp` + `quarkus.otel.metrics.exporter=otlp`.\n\nGenerate load: `make run-cli` \u2192 `account <id>` + `tables examples.iceberg`. Use *Component* and *Operation* filters + *Rate window* to slice workloads."
      }
    },

    {
      "type": "row",
      "title": "Service health",
      "id": 2,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 3 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Req/s",
      "id": 3,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 4 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(floecat_core_rpc_requests_total{component=~\"$component\"}[$rate_interval]))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "reqps", "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "yellow", "value": 50 }, { "color": "red", "value": 200 } ] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Avg latency (ms)",
      "id": 4,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 4, "w": 3, "x": 4, "y": 4 },
      "targets": [
        {
          "refId": "B",
          "expr": "1000 * (sum(rate(floecat_core_rpc_latency_seconds_sum{component=~\"$component\"}[$rate_interval])) / clamp_min(sum(rate(floecat_core_rpc_latency_seconds_count{component=~\"$component\"}[$rate_interval])), 1))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "ms", "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "yellow", "value": 50 }, { "color": "red", "value": 200 } ] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Error %",
      "id": 5,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 4, "w": 3, "x": 7, "y": 4 },
      "targets": [
        {
          "refId": "C",
          "expr": "100 * (sum(rate(floecat_core_rpc_errors_total{component=~\"$component\"}[$rate_interval])) / clamp_min(sum(rate(floecat_core_rpc_requests_total{component=~\"$component\"}[$rate_interval])), 1))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "percentunit", "decimals": 2, "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "yellow", "value": 0.01 }, { "color": "red", "value": 0.05 } ] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Active RPC",
      "id": 6,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 4, "w": 3, "x": 10, "y": 4 },
      "targets": [
        {
          "refId": "D",
          "expr": "sum(floecat_core_rpc_active{component=~\"$component\"})"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "short", "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "yellow", "value": 10 }, { "color": "red", "value": 50 } ] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Cache saturation",
      "id": 7,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 4, "w": 4, "x": 13, "y": 4 },
      "targets": [
        {
          "refId": "E",
          "expr": "max(100 * floecat_core_cache_entries{component=~\"$component\"} / clamp_min(floecat_core_cache_max_entries{component=~\"$component\"}, 1))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "percent", "decimals": 0, "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "yellow", "value": 70 }, { "color": "red", "value": 90 } ] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Heap used",
      "id": 8,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 4, "w": 4, "x": 17, "y": 4 },
      "targets": [
        {
          "refId": "F",
          "expr": "100 * (max(jvm_gc_live_data_size_bytes) / clamp_min(max(jvm_gc_max_data_size_bytes), 1))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "percent", "decimals": 0, "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "yellow", "value": 60 }, { "color": "red", "value": 80 } ] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Dropped tags",
      "id": 33,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 4, "w": 3, "x": 21, "y": 4 },
      "targets": [
        {
          "refId": "G",
          "expr": "sum(increase(floecat_core_observability_dropped_tags_total[$rate_interval]))"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0, "noValue": "0", "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "orange", "value": 1 }, { "color": "red", "value": 10 } ] } }, "overrides": [] },
      "description": "Tags dropped due to telemetry contract violations. Non-zero means a metric was emitted with disallowed or missing tags."
    },

    {
      "type": "row",
      "title": "RPC",
      "id": 9,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 8 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Requests by operation (req/s)",
      "id": 10,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 9 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (operation) (rate(floecat_core_rpc_requests_total{component=~\"$component\",operation=~\"$operation\"}[$rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Errors by status (req/s)",
      "id": 11,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 9 },
      "targets": [
        {
          "refId": "B",
          "expr": "sum by (status) (rate(floecat_core_rpc_errors_total{component=~\"$component\",operation=~\"$operation\"}[$rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Avg latency by operation (ms)",
      "id": 12,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "C",
          "expr": "1000 * (sum by (operation) (rate(floecat_core_rpc_latency_seconds_sum{component=~\"$component\",operation=~\"$operation\"}[$rate_interval])) / clamp_min(sum by (operation) (rate(floecat_core_rpc_latency_seconds_count{component=~\"$component\",operation=~\"$operation\"}[$rate_interval])), 1))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "RPC status distribution (req/s)",
      "id": 13,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "refId": "D",
          "expr": "sum by (status) (rate(floecat_core_rpc_requests_total{component=~\"$component\",operation=~\"$operation\"}[$rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] }
    },

    {
      "type": "row",
      "title": "Storage",
      "id": 14,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Store requests (req/s)",
      "id": 15,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (operation) (rate(floecat_core_store_requests_total{component=~\"$component\"}[$rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Store throughput (bytes/s)",
      "id": 34,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(floecat_core_store_bytes_total{component=~\"$component\"}[$rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Store latency (ms)",
      "id": 16,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 31 },
      "targets": [
        {
          "refId": "C",
          "expr": "1000 * (sum by (operation) (rate(floecat_core_store_latency_seconds_sum{component=~\"$component\"}[$rate_interval])) / clamp_min(sum by (operation) (rate(floecat_core_store_latency_seconds_count{component=~\"$component\"}[$rate_interval])), 1))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Store errors (req/s)",
      "id": 17,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 31 },
      "targets": [
        {
          "refId": "D",
          "expr": "sum(rate(floecat_core_store_errors_total{component=~\"$component\"}[$rate_interval]))"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] }
    },
    {
      "type": "table",
      "title": "Per-account storage",
      "id": 18,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 6, "x": 18, "y": 31 },
      "targets": [
        {
          "refId": "E",
          "expr": "floecat_service_storage_account_bytes",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true, "telemetry_contract_version": true },
            "renameByName": { "account": "Account", "Value": "Bytes" }
          }
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [ { "matcher": { "id": "byName", "options": "Account" }, "properties": [ { "id": "custom.width", "value": 150 } ] } ] }
    },

    {
      "type": "row",
      "title": "Cache",
      "id": 19,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 38 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Cache entries",
      "id": 20,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 39 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (cache) (floecat_core_cache_entries{component=~\"$component\"})"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Cache weight (bytes)",
      "id": 21,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 39 },
      "targets": [
        {
          "refId": "B",
          "expr": "sum by (cache) (floecat_core_cache_weighted_size_bytes{component=~\"$component\"})"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Cache hit rate (%)",
      "id": 22,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 46 },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * sum by (cache) (rate(floecat_core_cache_hits_total{component=~\"$component\"}[$rate_interval])) / clamp_min(sum by (cache) (rate(floecat_core_cache_hits_total{component=~\"$component\"}[$rate_interval])) + sum by (cache) (rate(floecat_core_cache_misses_total{component=~\"$component\"}[$rate_interval])), 1)",
          "legendFormat": "{{cache}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent", "min": 0, "max": 100, "thresholds": { "mode": "absolute", "steps": [ { "color": "red", "value": null }, { "color": "yellow", "value": 50 }, { "color": "green", "value": 90 } ] } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Cache enabled",
      "id": 23,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 46 },
      "targets": [
        {
          "refId": "D",
          "expr": "max by (cache) (floecat_core_cache_enabled{component=~\"$component\"})",
          "legendFormat": "{{cache}}"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "value_and_name", "colorMode": "value", "graphMode": "none", "justifyMode": "center" },
      "fieldConfig": { "defaults": { "unit": "short", "mappings": [ { "type": "value", "options": { "0": { "text": "Disabled" }, "1": { "text": "Enabled" } } } ], "thresholds": { "mode": "absolute", "steps": [ { "color": "red", "value": null }, { "color": "green", "value": 1 } ] } }, "overrides": [] }
    },

    {
      "type": "row",
      "title": "Background tasks",
      "id": 24,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 53 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Tasks running",
      "id": 25,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 54 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(floecat_core_task_running{component=~\"$component\"})"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Last task tick age (s)",
      "id": 26,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 54 },
      "targets": [
        {
          "refId": "B",
          "expr": "(time() * 1000 - max(floecat_core_task_last_tick_end_ms{component=~\"$component\"})) / 1000"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "s", "decimals": 0, "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "yellow", "value": 60 }, { "color": "red", "value": 300 } ] } }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "GC collections (rate by type)",
      "id": 27,
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": { "h": 5, "w": 12, "x": 12, "y": 54 },
      "targets": [
        {
          "refId": "C",
          "expr": "sum by (gc) (rate(floecat_core_gc_collections_total{component=~\"$component\"}[$rate_interval]))",
          "legendFormat": "{{gc}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },

    {
      "type": "row",
      "title": "System (JVM / host)",
      "id": 28,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 59 },
      "collapsed": true,
      "panels": [
        {
          "type": "timeseries",
          "title": "CPU usage",
          "id": 29,
          "datasource": "${DS_PROMETHEUS}",
          "gridPos": { "h": 7, "w": 12, "x": 0, "y": 60 },
          "targets": [
            { "refId": "A", "expr": "system_cpu_usage" },
            { "refId": "B", "expr": "process_cpu_usage" }
          ],
          "fieldConfig": { "defaults": { "unit": "percentunit" }, "overrides": [] }
        },
        {
          "type": "timeseries",
          "title": "Load avg (1m)",
          "id": 30,
          "datasource": "${DS_PROMETHEUS}",
          "gridPos": { "h": 7, "w": 12, "x": 12, "y": 60 },
          "targets": [
            { "refId": "C", "expr": "system_load_average_1m" }
          ],
          "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] }
        },
        {
          "type": "timeseries",
          "title": "Threads (live / daemon)",
          "id": 31,
          "datasource": "${DS_PROMETHEUS}",
          "gridPos": { "h": 7, "w": 12, "x": 0, "y": 67 },
          "targets": [
            { "refId": "D", "expr": "jvm_threads_live_threads" },
            { "refId": "E", "expr": "jvm_threads_daemon_threads" }
          ],
          "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] }
        },
        {
          "type": "timeseries",
          "title": "Open files",
          "id": 32,
          "datasource": "${DS_PROMETHEUS}",
          "gridPos": { "h": 7, "w": 12, "x": 12, "y": 67 },
          "targets": [
            { "refId": "F", "expr": "process_files_open_files" }
          ],
          "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] }
        }
      ]
    }
  ]
}
